{
  "permissions": [
    "activeTab",
	"storage",
	"contextMenus",
	"notifications"
  ],
  "browser_action": {
	"default_title": "CustomDiscord",
	"default_icon": "img/icon_256.png"
  },
  "background": {
    "persistent": true,
    "scripts": ["background/main.js", "background/listeners.js"]
  },
  "manifest_version": 2,
  "name": "Custom Discord",
  "version": "1.0",
  "web_accessible_resources": [
	"styles/themes.css",
	"styles/style.css"
  ],
  "description": "",
  "icons": {
    "16": "img/icon_16.png",
	"32": "img/icon_32.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png",
	"256": "img/icon_256.png"
  }
}
